<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>

	<class name="org.digijava.module.aim.dbentity.AmpActivityInternalId" table="AMP_ACTIVITY_INTERNAL_ID">
	 		  <cache usage="read-write" />
	 	 <id name="id" column="id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_ACTIVITY_INTERNAL_seq</param>
			</generator>
		</id>
		
	 	<many-to-one name="ampActivity" column="amp_activity_id" class="org.digijava.module.aim.dbentity.AmpActivity" 
					 cascade="none" not-null="true" />
					 
		<many-to-one name="organisation" column="amp_org_id" class="org.digijava.module.aim.dbentity.AmpOrganisation" 
					 cascade="none" not-null="true" />
					 
		<property name="internalId" column="internal_id" type="java.lang.String"/>

	</class>
</hibernate-mapping>
