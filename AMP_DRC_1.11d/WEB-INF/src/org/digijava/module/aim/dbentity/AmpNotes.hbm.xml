<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpNotes" table="AMP_NOTES">
		<cache usage="read-write" />
		<id name="ampNotesId" column="amp_notes_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_NOTES_seq</param>
			</generator>
		</id>
		<property name="name" column="name" type="java.lang.String"/>
		<property name="type" column="type" type="java.lang.String"/>
		<property name="description" column="description"  type="org.digijava.kernel.db.SafeClobType"/>
		<property name="language" column="language" type="java.lang.String"/>
		<property name="version" column="version" type="java.lang.String"/>
		<many-to-one name="ampActivityId" column="amp_activity_id" class="org.digijava.module.aim.dbentity.AmpActivity" cascade="none"/>
	</class>
</hibernate-mapping>
