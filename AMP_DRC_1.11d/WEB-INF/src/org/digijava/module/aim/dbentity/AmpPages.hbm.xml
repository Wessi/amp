<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
		  <class name="org.digijava.module.aim.dbentity.AmpPages" table="AMP_PAGES">
					 <cache usage="read-write" />
					 <id name="ampPageId" column="amp_page_id" type="java.lang.Long">
								<generator class="native">
										  <param name="sequence">AMP_PAGES_seq</param>
								</generator>
					 </id>
						
					 <property name="pageName" column="page_name" type="java.lang.String"/>
					 <property name="pageCode" column="page_code" type="java.lang.String"/>
					 <property name="ampTeamId" column="amp_Team_Id" type="java.lang.Long"/>

					 <set name="filters" table="AMP_PAGE_FILTERS" lazy="true">
								<cache usage="read-write"/>
								<key column="amp_page_id"/>
								<many-to-many class="org.digijava.module.aim.dbentity.AmpFilters" column="amp_filter_id"/>
					 </set>

		  </class>
</hibernate-mapping>
