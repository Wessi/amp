<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpComponentsIndicators" table="AMP_COMPONENTS_INDICATORS">
		<cache usage="read-write" />
		<id name="ampCompIndId" column="amp_component_indicator_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_COMP_IND_seq</param>
			</generator>
		</id>
		
		<property name="code" column="code" type="java.lang.String"/>
		<property name="name" column="name" type="java.lang.String"/>
		<property name="description" column="description" type="org.digijava.kernel.db.SafeClobType"/>
		
	</class>
</hibernate-mapping>
