<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpActivityDocument" table="AMP_ACTIVITY_DOCUMENT">
		<cache usage="read-write" />
		<id name="id" column="id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_ACTIVITY_DOCUMENT_seq</param>
			</generator> 
		</id>
		<property name="uuid" column="uuid"  type="java.lang.String"/>
		<property name="documentType" column="document_type"  type="java.lang.String"/>
		<many-to-one name="ampActivity" column="amp_activity_id" class="org.digijava.module.aim.dbentity.AmpActivity" cascade="none"/>
	</class>
</hibernate-mapping>	