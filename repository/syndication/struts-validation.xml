<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE form-validation PUBLIC "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.0//EN"           "http://jakarta.apache.org/commons/dtds/validator_1_0.dtd">
<form-validation>
	<global></global>
	<formset>
	      <!-- Syndication module -->
		<form name="collectorFeedForm">
			<field property="feedTitle" depends="required,maxlength">
				<arg0 key="display.syndication.feedTitle"/>
				<arg1 name="maxlength" key="${var:maxlength}" resource="false"/>
				<var>
					<var-name>maxlength</var-name>
					<var-value>255</var-value>
				</var>
			</field>
			<field property="feedUrl" depends="required,maxlength">
				<arg0 key="display.syndication.feedUrl"/>
				<arg1 name="maxlength" key="${var:maxlength}" resource="false"/>
				<var>
					<var-name>maxlength</var-name>
					<var-value>255</var-value>
				</var>
			</field>
			<field property="sourceName" depends="required,maxlength">
				<arg0 key="display.syndication.sourceName"/>
				<arg1 name="maxlength" key="${var:maxlength}" resource="false"/>
				<var>
					<var-name>maxlength</var-name>
					<var-value>255</var-value>
				</var>
			</field>
			<field property="sourceUrl" depends="required,maxlength">
				<arg0 key="display.syndication.sourceUrl"/>
				<arg1 name="maxlength" key="${var:maxlength}" resource="false"/>
				<var>
					<var-name>maxlength</var-name>
					<var-value>255</var-value>
				</var>
			</field>
		</form>
		<form name="publishedFeedForm">
			<field property="feedTitle" depends="required,maxlength">
				<arg0 key="display.syndication.feedTitle"/>
				<arg1 name="maxlength" key="${var:maxlength}" resource="false"/>
				<var>
					<var-name>maxlength</var-name>
					<var-value>255</var-value>
				</var>
			</field>
			<field property="selectedCountry" depends="required,maxlength">
				<arg0 key="display.syndication.selectedCountry"/>
				<arg1 name="maxlength" key="${var:maxlength}" resource="false"/>
				<var>
					<var-name>maxlength</var-name>
					<var-value>255</var-value>
				</var>
			</field>
			<field property="selectedLanguage" depends="required,maxlength">
				<arg0 key="display.syndication.selectedLanguage"/>
				<arg1 name="maxlength" key="${var:maxlength}" resource="false"/>
				<var>
					<var-name>maxlength</var-name>
					<var-value>255</var-value>
				</var>
			</field>
		</form>

		 <!-- End of syndication module -->
	</formset>
</form-validation>
