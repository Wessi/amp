<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
 <form-beans>
    <form-bean name="permissionForm" type="org.digijava.module.gateperm.form.PermissionForm" />
</form-beans> 
  
  <action-mappings>
  
  <action path="/gateperm/managePerm"	
			scope="session"
			type="org.digijava.module.gateperm.action.ManagePermission"
			name="permissionForm" input="/showLayout.do?layout=gateperm_editPermission"
			>
      <forward name="list" path="/showLayout.do?layout=gateperm_listPermissions" />
	</action>



  </action-mappings>
  
</struts-config>
