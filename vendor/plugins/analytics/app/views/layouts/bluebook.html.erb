<?xml version="1.0" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">

<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
	  <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
	  <title><%= render_page_title %></title>
	
	  <%= javascript_include_tag(:defaults, 'swfobject', 'jtip', 'jquery_expander', 'jquery_template', 'behavior/menu', :cache => false) %>
	  <%= stylesheet_link_tag('scaffold', 'odadata_menu', 'odadata', 'jtip', :media => :all, :cache => false) %>
	  <%# stylesheet_link_tag('print', :media => :print) %>
	  <!--[if lte IE 6]><%= stylesheet_link_tag 'iefixes' -%><![endif]-->
	  <!--[if lt IE 7]><%= javascript_include_tag 'ie6pngfix' %><![endif]-->
	  <%= yield :head %>
	</head>
	
	<body id="body" class="bluebook">
		<div id="content">
			<table id="header">
				<tr> 
					<td>
						<%= flashobject_tag "/images/ODAnicFlashLogo.swf", :size => "80x80" %>
					</td>
					<td>
						<img src="/images/ODAnicLogo.jpg" alt="ODAnic" width="489" height="128" border="0" />
					</td>
					<td>
						<%= flashobject_tag "/images/ODAnicFlags.swf", :size => "80x54" %>
					</td>
				</tr>
				<tr class="menuline"><td colspan="4" height="2"><hr /></td></tr>
				<tr>
					<td colspan="4" width="100%">
						<%= render :partial => 'layouts/navigation/bluebook' %>
						<hr />
					</td>
				</tr>
			</table>
			
			<%= flash_messages %>
			<%= yield :header %>
			<%= yield  %>
		</div>
	
		<%= render :partial => 'layouts/footer' %>
	</body>
</html>
