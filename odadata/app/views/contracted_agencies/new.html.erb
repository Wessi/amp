<center>
	<% odaform_for @contracted_agency do |f| %>
	<table width="740" class="list">
		<tr>
			<td class="title" colspan=2><%= lc(:title) %></td>
		</tr>
		<%= render :partial => 'form', :locals => {:f => f} %>
			<tr>
				<td><%= f.submit %></td>
			</tr>
		<% end %>
	</table>
	<p><b><%= link_to ll(:actions, :back), contracted_agencies_path %></b>&nbsp;</p>
</center>