<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.kernel.user.UserLoginInfo" table="DG_USER">
		<id column="ID" name="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">dg_entity_seq</param>
			</generator>
		</id>
		<property name="lastVisit" column="LAST_VISIT" type="java.util.Date"/>
		<property name="secondToLastVisit" column="SECOND_TO_LAST_VISIT" type="java.util.Date"/>
		<property name="numberOfSessions" column="NUMBER_OF_SESSIONS" type="long"/>
   </class>
</hibernate-mapping>
