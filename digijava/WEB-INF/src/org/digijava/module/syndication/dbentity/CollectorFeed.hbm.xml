<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>

	<class name="org.digijava.module.syndication.dbentity.CollectorFeed" table="DG_COLLECTOR_FEED">
		<id column="ID" name="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">dg_collector_feed_seq</param>
			</generator>
		</id>

		<property name="feedTitle"   column="FEED_TITLE"  type="java.lang.String"/>
		<property name="feedDescription"  column="FEED_DESCRIPTION"  type="java.lang.String"/>
		<property name="feedUrl"  column="FEED_URL"  type="java.lang.String"/>
		<property name="sourceName" column="SOURCE_NAME"  type="java.lang.String"/>
		<property name="sourceUrl"  column="SOURCE_URL"  type="java.lang.String"/>
		<property name="xmlData" column="XML_DATA"  type="org.digijava.kernel.db.SafeClobType"/>
		<property name="creationDate" column="CREATION_DATE"  type="java.util.Date"/>

        <set name="items" cascade="save-update" inverse="true">
            <key column="FEED_ID"/>
            <one-to-many class="org.digijava.module.syndication.dbentity.CollectorFeedItem" />
        </set>

	</class>

</hibernate-mapping>