<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpColumns" table="AMP_COLUMNS">
		<cache usage="read-write" />
		<id name="columnId" column="columnId" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_COLUMNS_seq</param>
			</generator>
		</id>
		<property column="columnName" name="columnName" type="java.lang.String"/>
		<property column="aliasName" name="aliasName" type="java.lang.String"/>

		<set name="reports" table="amp_report_columns" lazy="true">
			<cache usage="read-write"/>
			<key column="columnId"/>
			<many-to-many class="org.digijava.module.aim.dbentity.AmpReports" column="amp_report_id"/>
		</set>

	</class>
</hibernate-mapping>
