<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpRegionalFunding" table="AMP_REGIONAL_FUNDING">
		<cache usage="read-write" />
		<id name="ampRegionalFundingId" column="amp_regional_funding_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_REG_FUNDING_seq</param>
			</generator>
		</id>

		<property name="transactionType" column="transaction_type"  type="java.lang.Integer"/>
		<property name="adjustmentType" column="adjustment_type"  type="java.lang.Integer"/>
		<property name="transactionDate" column="transaction_date" type="java.util.Date"/>
		<property name="reportingDate" column="reporting_date" type="java.util.Date" />
		<property name="transactionAmount" column="transaction_amount"  type="java.lang.Double"/>
		<property name="expenditureCategory" column="exp_category" type="java.lang.String"/>		

		<many-to-one name="activity" column="activity_id" class="org.digijava.module.aim.dbentity.AmpActivity" cascade="none"/>
		<many-to-one name="reportingOrganization" column="rep_organization_id" 
			class="org.digijava.module.aim.dbentity.AmpOrganisation" cascade="none"/>		
		<many-to-one name="perspective" column="perspective_id" class="org.digijava.module.aim.dbentity.AmpPerspective" cascade="none"/>
		<many-to-one name="currency" column="currency_id" class="org.digijava.module.aim.dbentity.AmpCurrency" cascade="none"/>
		<many-to-one name="region" column="region_id" class="org.digijava.module.aim.dbentity.AmpRegion" not-null="true"/>
	</class>
</hibernate-mapping>
