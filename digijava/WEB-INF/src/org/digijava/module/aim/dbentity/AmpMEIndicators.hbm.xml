<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpMEIndicators" table="AMP_ME_INDICATORS">
		<cache usage="read-write" />
		<id name="ampMEIndId" column="amp_me_indicator_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_ME_IND_seq</param>
			</generator>
		</id>
		
		<property name="code" column="code" type="java.lang.String"/>
		<property name="name" column="name" type="java.lang.String"/>
		<property name="type" column="type" type="java.lang.String"/>
		<property name="description" column="description" type="org.digijava.kernel.db.SafeClobType"/>
		<property name="defaultInd" column="default_ind" type="java.lang.Boolean"/>
		<property name="ascendingInd" column="ASCENDING_IND" type="java.lang.Boolean"/>		
		<property name="valueType" column="value_type"  type="java.lang.Integer"/>
		<property name="creationDate" column="creation_date" type="java.util.Date"/>
		<property name="indicatorCategory" column="indicator_category"  type="java.lang.Integer"/>
		<property name="npIndicator" column="np_indicator" type="java.lang.Boolean"/>
		<set name="themes" table="AMP_THEME_INDICATOR" lazy="true" cascade="none">
			<cache usage="read-write"/>
			<key column="amp_me_indicator_id"/>
			<many-to-many class="org.digijava.module.aim.dbentity.AmpTheme" column="amp_theme_id"/>
		</set>
		
	</class>
</hibernate-mapping>
