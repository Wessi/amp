<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpOrgGroup" table="AMP_ORG_GROUP">
		<cache usage="read-write" />
		<id name="ampOrgGrpId" column="amp_org_grp_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_ORG_GROUP_seq</param>
			</generator>
		</id>
		<property name="orgGrpName" column="org_grp_name" type="java.lang.String"/>
		<property name="orgGrpCode" column="org_grp_code" type="java.lang.String"/>

		<many-to-one name="levelId" column="amp_level_id" 
		class="org.digijava.module.aim.dbentity.AmpLevel" cascade="none"/>
		
		<!--
		<set name="organizations" cascade="delete" inverse="true">
		<cache usage="read-write" />
			<key column="amp_org_grp_id"/>
			<one-to-many class="org.digijava.module.aim.dbentity.AmpOrganisation"/>
		</set>
		-->
				
	</class>
</hibernate-mapping>
