<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpTeamMember" table="AMP_TEAM_MEMBER">
		<cache usage="read-write" />
		<id name="ampTeamMemId" column="amp_team_mem_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_TEAMMEM_seq</param>
			</generator>
		</id>

		<property name="readPermission" column="read_permission"  type="java.lang.Boolean"/>
		<property name="writePermission" column="write_permission"  type="java.lang.Boolean"/>
		<property name="deletePermission" column="delete_permission"  type="java.lang.Boolean"/>

		<many-to-one name="user" class="org.digijava.kernel.user.User" cascade="none"/>
		<many-to-one name="ampTeam" column="amp_team_id" class="org.digijava.module.aim.dbentity.AmpTeam" cascade="none"/>
		<many-to-one name="ampMemberRole" column="amp_member_role_id" class="org.digijava.module.aim.dbentity.AmpTeamMemberRoles" cascade="none"/>

		<set name="activities" table="amp_member_activities" lazy="true">
  	            <cache usage="read-write"/>
				<key column="amp_member_id"/>
				<many-to-many class="org.digijava.module.aim.dbentity.AmpActivity" column="amp_activity_id"/>
	 </set>	


		<set name="reports" table="amp_member_reports" lazy="true">
  	            <cache usage="read-write"/>
				<key column="amp_member_id"/>
				<many-to-many class="org.digijava.module.aim.dbentity.AmpReports" column="amp_report_id"/>
	 </set>		 
	</class>
</hibernate-mapping>
