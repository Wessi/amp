<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpOrgRole" table="AMP_ORG_ROLE">
		<cache usage="read-write" />
		<id name="ampOrgRoleId" column="amp_org_role_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_ORGROLE_seq</param>
			</generator>
		</id>
			  
		<many-to-one name="activity" class="org.digijava.module.aim.dbentity.AmpActivity" cascade="none"/>
		<many-to-one name="organisation" class="org.digijava.module.aim.dbentity.AmpOrganisation" cascade="none"/>			  
		<many-to-one name="role" class="org.digijava.module.aim.dbentity.AmpRole" cascade="none"/>				  
		
	</class>
</hibernate-mapping>

