<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.forum.dbentity.ForumAsset" table="DG_FORUM_ASSET">
		<cache usage="read-write"/>
		<id column="ID" name="id" type="long" unsaved-value="0">
			<generator class="native">
				<param name="sequence">dg_forum_asset_seq</param>
			</generator>
		</id>
		<property name="srcFileName" column="FILE_NAME" type="java.lang.String"/>
		<property name="fileSize" column="FILE_SIZE" type="long"/>
		<property name="forumUserId" column="AUTOR_USER" type="long"/>
		<property name="contentType" column="CONTENT_TYPE" type="java.lang.String"/>
		<property name="creationDate" column="CREATION_DATE" type="java.util.Date"/>
		<property name="fileContent" column="FILE_CONTENT" type="org.digijava.kernel.db.SafeBlobType"/>
	</class>
</hibernate-mapping>