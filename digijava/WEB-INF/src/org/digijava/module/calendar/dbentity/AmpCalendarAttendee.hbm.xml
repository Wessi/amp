<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.calendar.dbentity.AmpCalendarAttendee" table="AMP_CALENDAR_ATTENDEE">
		<id column="ID" name="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">calendar_attendee_seq</param>
			</generator>
		</id>
        <many-to-one name="ampCalendar" column="CALENDAR_ID" class="org.digijava.module.calendar.dbentity.AmpCalendar"/>
		<many-to-one name="user" column="USER_ID" class="org.digijava.kernel.user.User"/>
        <property name="guest" column="GUEST"  type="java.lang.String"/>
	</class>
</hibernate-mapping>
