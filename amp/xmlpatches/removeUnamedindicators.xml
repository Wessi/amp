<?xml version="1.0" encoding="UTF-8"?>
<tns:patch closeOnSuccess="true" retryOnFail="true"
	xmlns:tns="http://docs.ampdev.net/schemas/xmlpatcher" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://docs.ampdev.net/schemas/xmlpatcher ../doc/xmlpatcher.xsd ">
	<jira>AMP-10532</jira>
	<keyword>Indicator</keyword>
	<author>Medea Gugushvili</author>
	<description>Remove Indicators without name</description>
	<apply>
		<script>
			<lang delimiter=";" type="postgres">
    			DELETE FROM amp_indicator_connection where indicator_id in (SELECT indicator_id FROM amp_indicator WHERE name is null);
				DELETE FROM amp_indicator WHERE name is null;
            </lang>
		</script>
	</apply>
</tns:patch>
