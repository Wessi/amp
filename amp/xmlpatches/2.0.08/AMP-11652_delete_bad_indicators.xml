<?xml version="1.0" encoding="UTF-8"?>
<tns:patch closeOnSuccess="true" retryOnFail="true" xmlns:tns="http://docs.ampdev.net/schemas/xmlpatcher" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://docs.ampdev.net/schemas/xmlpatcher ../doc/xmlpatcher.xsd ">
    <jira>AMP-11652</jira>
    <keyword>Indicators</keyword>
    <author>Dare</author>
    <description>Delete duplicate records</description> 
    <apply>
        <script>
            <lang delimiter=";" type="postgres">
              
			delete from amp_indicator_connection where indicator_id in 
(select indicator_id from amp_indicator where name is null or trim(name)='') ;

delete from amp_indicator where name is null or trim(name)='';
            </lang>
        </script>
    </apply>
</tns:patch>