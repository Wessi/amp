<?xml version="1.0" encoding="UTF-8"?>
<tns:patch closeOnSuccess="true" retryOnFail="true" xmlns:tns="http://docs.ampdev.net/schemas/xmlpatcher" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://docs.ampdev.net/schemas/xmlpatcher ../doc/xmlpatcher.xsd ">
    <jira>AMP-11783</jira>
    <keyword>Messages</keyword>
    <author>Dare</author>
    <description>Updated Object_url for messages</description> 
    <apply>
        <script>
            <lang delimiter=";" type="postgres">              
			update amp_message set object_url='/aim/viewActivityPreview.do~public=true~pageId=2~activityId=' || related_act_id where
object_url like '%/aim/viewChannelOverview.do%';
            </lang>
        </script>
    </apply>
</tns:patch>






