<?xml version="1.0" encoding="UTF-8"?>
<tns:patch closeOnSuccess="true" retryOnFail="true"
	xmlns:tns="http://docs.ampdev.net/schemas/xmlpatcher" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://docs.ampdev.net/schemas/xmlpatcher ../doc/xmlpatcher.xsd ">
	<jira>AMP-16737</jira>
	<keyword>Add Date to Measure</keyword>
	<author>emaperez</author>
	<description>Add Date to Measures in Issues Section on Activity Form</description>
	<apply>
		<script>
			<lang delimiter=";" type="postgres">
				ALTER TABLE amp_measure ADD COLUMN measureDate timestamp without time zone;
       		</lang>
		</script>
	</apply>
</tns:patch>