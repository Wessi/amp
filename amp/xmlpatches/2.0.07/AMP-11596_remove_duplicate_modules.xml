<?xml version="1.0" encoding="UTF-8"?>
<tns:patch closeOnSuccess="true" retryOnFail="true" xmlns:tns="http://docs.ampdev.net/schemas/xmlpatcher" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://docs.ampdev.net/schemas/xmlpatcher ../doc/xmlpatcher.xsd ">
    <jira>AMP-11596</jira>
    <keyword>FM</keyword>
    <author>Dare</author>
    <description>Delete duplicate records</description> 
    <apply>
        <script>
            <lang delimiter=";" type="postgres">
               delete from amp_modules_visibility where name like 'Content Repository' and id not in 
(select parent from amp_features_visibility where name in ('My Resources','Team Resources','Public Resources','Shared Resources'));
            </lang>
        </script>
    </apply>
</tns:patch>