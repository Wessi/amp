<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.kernel.entity.PrincipalPermissionParameter" table="DG_PRINCIPAL_PERMISSION_PARAM">
		<composite-id>
			<key-many-to-one name="principalPermission" class="org.digijava.kernel.entity.PrincipalPermission" column="PRINCIPAL_PERMISSION_ID"/>
     		<key-property name="index" column="PARAM_INDEX" type="int"/>
		</composite-id>
		<property name="parameterClass" column="PARAM_CLASS" type="java.lang.String"/>
		<property name="parameterValue" column="PARAM_VALUE" type="org.digijava.kernel.db.SafeClobType"/>
   </class>
</hibernate-mapping>
