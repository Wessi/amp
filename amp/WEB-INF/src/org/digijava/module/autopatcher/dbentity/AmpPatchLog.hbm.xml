<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.autopatcher.dbentity.AmpPatchLog" table="AMP_PATCH_LOG">
		<cache usage="read-write" />
		<id name="id" column="id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_PATCH_LOG_seq</param>
			</generator>
		</id>

	<many-to-one name="patch" column="amp_patch_id" class="org.digijava.module.autopatcher.dbentity.AmpPatch" 
					 cascade="none" not-null="true" />
	
		<property name="invoked" column="invoked" type="java.sql.Timestamp"/>
		<property name="elapsed" column="elapsed" type="java.lang.Long"/>
		<property name="sql" column="sql" type="java.lang.Long"/>	
		<property name="response" column="response" type="java.lang.Long"/>			
		<property name="actionName" column="actionName" type="java.lang.Long"/>		
		<property name="actionReason" column="actionReason" type="java.lang.Long"/>	
		<property name="successful" column="successful" type="java.lang.Boolean"/>			
		
		<set name="logs" table="AMP_PATCH_LOG" cascade="all">
			<cache usage="read-write"/>
			<key column="amp_patch_id"/>
			<one-to-many class="org.digijava.module.aim.dbentity.AmpSector" />
		</set>
		
		
</class>