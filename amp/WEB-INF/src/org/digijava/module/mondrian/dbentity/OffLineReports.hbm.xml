<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.mondrian.dbentity.OffLineReports" table="off_line_reports">
		<id name="Id" column="id" type="java.lang.Long"
			unsaved-value="null">
			<generator class="native">
				<param name="sequence">Off_Line_Reports_seq</param>
			</generator>
		</id>

		<property name="name" column="name" type="java.lang.String" />
		<property name="query" column="query" type="text" />
		<property name="teamid" column="teamid" type="java.lang.Long"/>
		<property name="publicreport" column="ispublic" type="java.lang.Boolean"/>
		<property name="measures" column="measures" type="java.lang.String"/>
		<property name="columns" column="columns" type="java.lang.String"/>
		<property name="creationdate" column="creationdate" type="java.sql.Timestamp"/>
		<property name="type" column="type" type="java.lang.Integer"/>
		<many-to-one name="ownerId" column="ownerId" class="org.digijava.module.aim.dbentity.AmpTeamMember" />
	</class>
</hibernate-mapping>