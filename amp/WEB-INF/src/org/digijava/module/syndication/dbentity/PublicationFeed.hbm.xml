<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>

	<class name="org.digijava.module.syndication.dbentity.PublicationFeed" table="DG_PUBLICATION_FEED">
		<id column="ID" name="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">dg_publication_feed_seq</param>
			</generator>
		</id>

		<property name="siteId" column="SITE_ID"  type="java.lang.String"/>
		<property name="contentType" column="CONTENT_TYPE"  type="java.lang.String"/>
		<property name="instanceId" column="MODULE_INSTANCE_ID"  type="java.lang.String"/>
		<property name="feedTitle"   column="FEED_TITLE"  type="java.lang.String"/>
		<property name="feedDescription"  column="FEED_DESCRIPTION"  type="java.lang.String"/>
		<property name="feedUrl"  column="FEED_URL"  type="java.lang.String"/>
		<property name="status"  column="STATUS"  type="boolean"/>
		<many-to-one name="language" column="LANGUAGE" class="org.digijava.kernel.entity.Locale" cascade="none"/>
		<many-to-one name="country" column="COUNTRY_ISO" class="org.digijava.kernel.dbentity.Country" cascade="none"/>
		<property name="creationDate" column="CREATION_DATE"  type="java.util.Date"/>

	</class>

</hibernate-mapping>