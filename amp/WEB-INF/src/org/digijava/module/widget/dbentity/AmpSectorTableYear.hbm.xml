<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class discriminator-value="secYear" name="org.digijava.module.widget.dbentity.AmpSectorTableYear" table="AMP_WIDGET_SECTOR_TABLE_YEAR">
    <cache usage="read-write"/>
    <id column="ID" name="id" type="java.lang.Long" unsaved-value="null">
      <generator class="native">
        <param name="sequence">AMP_WIDGET_SECTOR_TABLE_YEAR_seq</param>
      </generator>
    </id>
    <property name="year" not-null="true" type="java.lang.Long"/>
    <property name="type" not-null="true" type="java.lang.Long"/>
    <property column="column_order" name="order" type="java.lang.Long"/>
    <many-to-one cascade="none" class="org.digijava.module.widget.dbentity.AmpSectorTableWidget" column="sector_widget_table_id" name="widget"/>
  </class>
</hibernate-mapping>
