<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.widget.dbentity.AmpSectorTableYear" table="AMP_WIDGET_SECTOR_TABLE_YEAR"
		discriminator-value="secYear">
		<cache usage="read-write" />
		<id column="ID" name="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">AMP_WIDGET_SECTOR_TABLE_YEAR_seq</param>
			</generator>
		</id>
		<property name="year"  type="java.lang.Long" not-null="true"/>
        <property name="type"  type="java.lang.Long"  not-null="true"/>
        <property name="order"  type="java.lang.Long"  column="column_order"/>
		<many-to-one name="widget" column="sector_widget_table_id"
			class="org.digijava.module.widget.dbentity.AmpSectorTableWidget" cascade="none" />
	</class>
</hibernate-mapping>
