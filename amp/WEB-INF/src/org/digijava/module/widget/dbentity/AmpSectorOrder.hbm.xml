<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class discriminator-value="secOrd" name="org.digijava.module.widget.dbentity.AmpSectorOrder" table="AMP_WIDGET_SECTOR_ORDER">
    <cache usage="read-write"/>
    <id column="ID" name="id" type="java.lang.Long" unsaved-value="null">
      <generator class="native">
        <param name="sequence">AMP_WIDGET_SECTOR_ORDER_seq</param>
      </generator>
    </id>
    <property column="order_number" name="order" type="int"/>
    <many-to-one cascade="none" class="org.digijava.module.widget.dbentity.AmpSectorTableWidget" column="sector_widget_table_id" name="widget"/>
    <many-to-one cascade="none" class="org.digijava.module.aim.dbentity.AmpSector" column="sector_id" name="sector"/>
  </class>
</hibernate-mapping>
