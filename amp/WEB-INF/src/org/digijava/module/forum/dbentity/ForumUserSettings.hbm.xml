<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.forum.dbentity.ForumUserSettings" table="DG_FORUM_USER_SETTINGS">
		<cache usage="read-write"/>
		<id column="ID" name="id" type="long" unsaved-value="0">
			<generator class="native">
				<param name="sequence">dg_forum_user_settings_seq</param>
			</generator>
		</id>
		<property name="digiUserId" column="DIGI_USER_ID" type="long"/>
		<property name="forumId" column="FORUM_ID" type="long"/>
		<property name="nickName" column="NICK_NAME" type="java.lang.String"/>
		<property name="signature" column="SIGNATURE" type="java.lang.String"/>
		<property name="totalPosts" column="TOTAL_POSTS" type="int"/>
		<property name="avatarUrl" column="AVATAR_URL" type="java.lang.String"/>
		<property name="lastActiveTime" column="LAST_ACTIVE_TIME" type="java.util.Date"/>
		<property name="registerDate" column="REGISTER_DATE" type="java.util.Date"/>
		<property name="lastPmNotify" column="LAST_PM_NOTIFY" type="java.util.Date"/>
		<property name="location" column="LOCATION" type="java.lang.String"/>
	</class>
</hibernate-mapping>
