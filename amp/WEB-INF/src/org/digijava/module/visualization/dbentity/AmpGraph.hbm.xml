<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.visualization.dbentity.AmpGraph" table="amp_graph">
		<id name="id" column="id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">amp_graph_seq</param>
			</generator>
		</id>
	
		<property name="name" column="name" type="java.lang.String" />
		<property name="extractMethod" column="extract_method" type="java.lang.String" />
		<property name="barGraphEnabled" column="bar_graph_enabled" type="java.lang.Boolean" />
		<property name="barProfileGraphEnabled" column="bar_profile_graph_enabled" type="java.lang.Boolean" />
		<property name="lineGraphEnabled" column="line_graph_enabled" type="java.lang.Boolean" />
		<property name="pieGraphEnabled" column="pie_graph_enabled" type="java.lang.Boolean" />
		<property name="dataListEnabled" column="data_list_enabled" type="java.lang.Boolean" />
		
 	 </class>
</hibernate-mapping>