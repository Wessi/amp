<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpWoreda" table="AMP_WOREDA">
		<cache usage="read-write" />
		<id name="ampWoredaId" column="amp_woreda_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_WOREDA_seq</param>
			</generator>
		</id>

		<property name="name" column="name" type="java.lang.String" />

		<property name="description" column="description" type="text" />
		<property name="gsLat" column="gs_lat" type="java.lang.String" />
		<property name="gsLong" column="gs_long" type="java.lang.String" />
		<property name="geoCode" column="geo_code" type="java.lang.String" />
		<property name="woredaCode" column="woreda_code" type="java.lang.String" />
		<many-to-one name="country" column="country_id" class="org.digijava.kernel.dbentity.Country"
			cascade="none" />
		<many-to-one name="zone" column="zone_id" class="org.digijava.module.aim.dbentity.AmpZone"
			cascade="none" />
		<set name="locations" cascade="all">
			<key column="woreda_id" />
			<one-to-many class="org.digijava.module.aim.dbentity.AmpLocation" />
		</set>

	</class>
</hibernate-mapping>
