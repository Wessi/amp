<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.digijava.module.aim.dbentity.AmpActivityComponente"
		table="AMP_ACTIVITY_COMPONENTE">
		<cache usage="read-write" />
		<id name="id" column="amp_activity_componente_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_ACTIVITY_COMPONENTE_seq</param>
			</generator>
		</id>

		<many-to-one name="activity" column="amp_activity_id"
			class="org.digijava.module.aim.dbentity.AmpActivity" cascade="none" not-null="true" />

		<many-to-one name="sector" column="amp_sector_id"
			class="org.digijava.module.aim.dbentity.AmpSector" cascade="none" not-null="true" />

		<property name="percentage" column="percentage" type="java.lang.Float" />


	</class>

</hibernate-mapping>
