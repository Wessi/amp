<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpResourceManagerSettings" table="AMP_RESOURCE_MANAGER_SETTINGS">
		<cache usage="read-write" />
		<id name="ampResourceManagerSettingsId" column="id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_RESOURCE_MANAGER_SETTINGS_seq</param>
			</generator>
		</id>

		
		<property name="sortColumn" column="sort_column" type="java.lang.String" />
		<property name="maximunFileSize" column="maximun_file_size" type="java.lang.Integer" />
		<property name="limitFileToUpload" column="limit_file_to_upload" type="java.lang.Boolean" />
	</class>
</hibernate-mapping>
