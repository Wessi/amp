<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpCurrencyRate" table="AMP_CURRENCY_RATE">
		<cache usage="read-write" />
		<id name="ampCurrencyRateId" column="amp_currency_rate_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_CURRENCY_RATE_seq</param>
			</generator>
		</id>

		<property column="from_currency_code" name="fromCurrencyCode" type="java.lang.String" />
		<property column="to_currency_code" name="toCurrencyCode" type="java.lang.String" />

		<property name="exchangeRate" column="exchange_rate" type="java.lang.Double" />
		<property name="exchangeRateDate" column="exchange_rate_date" type="java.util.Date" />
		<property name="dataSource" column="data_source" type="java.lang.Integer" />


	</class>
</hibernate-mapping>
