<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.IPAContractDisbursement" table="IPA_CONTRACT_DISBURSEMENT">
	<cache usage="read-write" />
	<id name="id" column="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">IPA_CONTRACT_DISBURSEMENT_seq</param>
			</generator>
	</id>
	<property name="adjustmentType" column="adjustment_type" type="java.lang.Integer"/>
	<property access="field" name="amount" column="amount" type="java.lang.Double"/>
	<many-to-one name="currency" column="currency" class="org.digijava.module.aim.dbentity.AmpCurrency" cascade="none"/>
        <many-to-one name="contract" column="ipa_contract_id" class="org.digijava.module.aim.dbentity.IPAContract" cascade="save-update"/>		
	<property name="date" column="date" type="java.util.Date"/>	
	</class>
</hibernate-mapping>