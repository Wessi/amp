<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpLevel" table="AMP_LEVEL">
		<cache usage="read-write" />
		<id name="ampLevelId" column="amp_level_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_LEVEL_seq</param>
			</generator>
		</id>
		<property name="levelCode" column="level_code" type="java.lang.String" />
		<property name="name" column="name" type="java.lang.String" />
		<property name="levelType" column="type" type="java.lang.String" />
		<property name="description" column="description" type="text" />
		<property name="language" column="language" type="java.lang.String" />
		<property name="version" column="version" type="java.lang.String" />
	</class>
</hibernate-mapping>






