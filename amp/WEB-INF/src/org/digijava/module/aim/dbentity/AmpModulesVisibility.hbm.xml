<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpModulesVisibility" table="AMP_MODULES_VISIBILITY">
		<cache usage="transactional"/>
		<id name="id" column="id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_FIELD_seq</param>
			</generator>
		</id>
		<property name="name" column="name" type="java.lang.String" />
		
		
	 <set name="items" table="AMP_FEATURES_VISIBILITY" cascade="all">
      	   <cache usage="read-write" />
           <key column="parent"/>
           <one-to-many class="org.digijava.module.aim.dbentity.AmpFeaturesVisibility"/>
     </set>
     
     <many-to-one name="parent" column="parent" class="org.digijava.module.aim.dbentity.AmpModulesVisibility"/>
     
     
     <set name="submodules" cascade="all" inverse="true">
           <key column="parent"/>
           <one-to-many class="org.digijava.module.aim.dbentity.AmpModulesVisibility"/>
     </set>
     
    <set name="templates" table="amp_modules_templates" inverse="true">
		   <cache usage="read-write" />
           <key column="module"/>
           <many-to-many column="template" class="org.digijava.module.aim.dbentity.AmpTemplatesVisibility"/>
     </set>
		
	</class>
</hibernate-mapping>
