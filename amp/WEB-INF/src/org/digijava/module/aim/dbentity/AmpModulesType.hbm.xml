<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpModulesType" table="AMP_MODULES_TYPE">
		<cache usage="read-write" />
		<id name="id" column="id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_FIELD_seq</param>
			</generator>
		</id>
		<property name="name" column="name" type="java.lang.String" />
		
		<set name="modules" table="AMP_MODULES_TYPE" cascade="all" inverse="true">
      	   <cache usage="read-write" />
           <key column="type"/>
           <one-to-many class="org.digijava.module.aim.dbentity.AmpModulesVisibility"/>
	    </set>
	    
	</class>
</hibernate-mapping>
