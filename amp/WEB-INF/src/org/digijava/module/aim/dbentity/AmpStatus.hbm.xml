<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpStatus" table="AMP_STATUS">
		<id name="ampStatusId" column="amp_status_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_STATUS_seq</param>
			</generator>
		</id>
		<property column="status_code" name="statusCode" type="java.lang.String" />
		<property column="type" name="type" type="java.lang.String" />
		<property column="name" name="name" type="java.lang.String" />
		<property column="description" name="description" type="text" />
		<property column="language" name="language" type="java.lang.String" />
		<property column="version" name="version" type="java.lang.String" />
	</class>
</hibernate-mapping>






