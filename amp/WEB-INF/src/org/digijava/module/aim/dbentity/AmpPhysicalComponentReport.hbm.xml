<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpPhysicalComponentReport"
		table="AMP_PHYSICAL_COMPONENT_REPORT">
		<cache usage="read-write" />
		<id name="ampReportId" column="amp_report_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_PHYSICAL_COMP_REP_seq</param>
			</generator>
		</id>
		<property name="ampActivityId" column="amp_activity_id" type="java.lang.Long" />
		<property name="activityName" column="activity_name" type="java.lang.String" />
		<property name="ampDonorId" column="amp_donor_id" type="java.lang.Long" />
		<property name="donorName" column="donor_name" type="java.lang.String" />
		<property name="ampComponentId" column="amp_component_id" type="java.lang.Long" />
		<property name="componentName" column="component_name" type="java.lang.String" />
		<property name="objective" column="objective" type="text" />
		<property name="signatureDate" column="signature_date" type="java.util.Date" />
		<property name="actualStartDate" column="actual_start_date" type="java.util.Date" />
		<property name="plannedCompletionDate" column="planned_completion_date"
			type="java.util.Date" />
		<property access="field" name="actualCommitment" column="actual_commitment"
			type="java.math.BigDecimal" />
		<property name="commCurrencyCode" column="comm_currency_code" type="java.lang.String" />
		<property access="field" name="actualExpenditure" column="actual_expenditure"
			type="java.math.BigDecimal" />
		<property name="expCurrencyCode" column="exp_currency_code" type="java.lang.String" />
		<property name="status" column="status" type="java.lang.String" />
		<property name="reportingDate" column="reporting_date" type="java.util.Date" />
		<property name="mofedContact" column="mofed_contact" type="text" />
		<property name="donorContact" column="donor_contact" type="text" />
		<property name="ampTeamId" column="amp_team_id" type="java.lang.Long" />
		<property name="transactionDate" column="transaction_date" type="java.util.Date" />
		<property name="fiscalYear" column="fiscal_year" type="java.lang.Integer" />
		<property name="ampStatusId" column="amp_status_id" type="java.lang.Long" />
		<property name="ampModalityId" column="amp_modality_id" type="java.lang.Long" />

	</class>
</hibernate-mapping>
