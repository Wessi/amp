<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpReportSector" table="AMP_REPORT_SECTOR">
		<cache usage="read-write" />
		<id name="ampReportId" column="amp_report_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_REPORT_SECTOR_seq</param>
			</generator>
		</id>
		<property name="ampActivityId" column="amp_activity_id" type="java.lang.Long" />
		<property name="sectorName" column="sector_name" type="java.lang.String" />
		<property name="ampSectorId" column="amp_sector_id" type="java.lang.Long" />
		<property name="sectorCode" column="sector_code" type="java.lang.String" />
		<property name="ampSubSectorId" column="amp_sub_sector_id" type="java.lang.Long" />
		<property name="subSectorName" column="sub_sector_name" type="java.lang.String" />

	</class>
</hibernate-mapping>
