<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpPerspective" 
	table="AMP_PERSPECTIVE">
		<cache usage="read-write" />
		<id name="ampPerspectiveId" column="amp_perspective_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_PERSPECTIVE_seq</param>
			</generator>
		</id>
		<property name="name" column="name" type="java.lang.String"/>
		<property name="code" column="code" type="java.lang.String"/>
	</class>
</hibernate-mapping>
