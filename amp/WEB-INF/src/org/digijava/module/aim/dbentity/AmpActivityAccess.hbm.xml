<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpActivityAccess" table="AMP_ACTIVITY_ACCESS">
	<cache usage="read-write" />
  	<id name="id" column="id" type="java.lang.Long">
		<generator class="native">
			<param name="sequence">AMP_ACTIVITY_ACCESS_seq</param>
		</generator>
	</id>
	
	
	<property name="viewed" column="viewed" type="java.lang.Boolean" />
	<property name="updated" column="updated" type="java.lang.Boolean" />
	<property name="changeDate" column="change_date" type="java.util.Date" />
	
	<many-to-one name="activity" column="amp_activity_id"	class="org.digijava.module.aim.dbentity.AmpActivityVersion" cascade="none" lazy="false"/>
	<many-to-one name="user"  column="dg_user_id"	class="org.digijava.kernel.user.User" cascade="none" lazy="false"/>

  </class>
</hibernate-mapping>