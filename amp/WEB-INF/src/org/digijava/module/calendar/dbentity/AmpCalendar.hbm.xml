<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.calendar.dbentity.AmpCalendar" table="AMP_CALENDAR">
        <composite-id name="calendarPK" class="org.digijava.module.calendar.dbentity.AmpCalendarPK">
            <key-many-to-one column="CALENDAR_ID" name="calendar" class="org.digijava.module.calendar.dbentity.Calendar"/>
        </composite-id>
        <many-to-one column="EVENT_TYPE_ID" name="eventType" class="org.digijava.module.calendar.dbentity.AmpEventType"/>
        <many-to-one column="USER_ID" name="user" class="org.digijava.kernel.user.User"/>
        <set name="donors" table="AMP_CALENDAR_DONORS_MAP" lazy="true">
            <key column="CALENDAR_ID"/>
            <many-to-many class="org.digijava.module.aim.dbentity.AmpOrganisation" column="amp_org_id"/>
        </set>
        <set name="attendees" cascade="all" inverse="true" lazy="true">
            <key column="CALENDAR_ID"/>
            <one-to-many class="org.digijava.module.calendar.dbentity.AmpCalendarAttendee"/>
        </set>
        <property name="privateEvent" column="PRIVATE" type="boolean"/>
	</class>
</hibernate-mapping>
