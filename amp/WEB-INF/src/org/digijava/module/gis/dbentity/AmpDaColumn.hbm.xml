<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.gis.dbentity.AmpDaColumn" table="AMP_DA_COLUMN">
		<cache usage="read-write"/>
		<id column="ID" name="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">AMP_DA_COLUMN_seq</param>
			</generator>
		</id>

        <property name="name" column="col_name" type="java.lang.String"/>
		<property name="code" column="col_code" type="java.lang.String"/>
		<property name="cssClass" column="css_class" type="java.lang.String"/>
		<property name="htmlStyle" column="html_style" type="java.lang.String"/>
		<property name="pattern" column="display_pattern" type="java.lang.String"/>
		<property name="width" column="col_width" type="java.lang.String"/>
		<property name="orderNo" column="order_no" type="java.lang.Integer"/>

		<many-to-one name="widget" column="widget_id" class="org.digijava.module.gis.dbentity.AmpDaTable" cascade="none"/>		
		
	</class>
</hibernate-mapping>
