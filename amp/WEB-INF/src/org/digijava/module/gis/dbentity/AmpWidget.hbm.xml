<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.gis.dbentity.AmpWidget" table="AMP_WIDGET" discriminator-value="w">
		<cache usage="read-write"/>
		<id column="ID" name="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">AMP_WIDGET_seq</param>
			</generator>
		</id>

		<discriminator column="widget_type"/>
        <property name="name" column="widget_name" type="java.lang.String"/>
		<property name="code" column="widget_code" type="java.lang.String"/>
		

		<subclass name="org.digijava.module.gis.dbentity.AmpWidgetIndicatorChart" discriminator-value="indsecchart">
			<many-to-one name="indicator" column="ind_connect_id" class="org.digijava.module.aim.dbentity.IndicatorSector" cascade="none"/> 
		</subclass>			

		<subclass name="org.digijava.module.gis.dbentity.AmpDaTable" discriminator-value="tbl">
			<property name="cssClass" column="css_class" type="java.lang.String"/>
			<property name="htmlStyle" column="html_style" type="java.lang.String"/>
			<property name="width" column="table_width" type="java.lang.String"/>
			<set name="columns" lazy="true" inverse="true" order-by="order_no asc" cascade="all-delete-orphan">
				<key column="table_widget_id"/>
				<one-to-many class="org.digijava.module.gis.dbentity.AmpDaColumn"/>
			</set>
		</subclass>			

		
	</class>
</hibernate-mapping>
