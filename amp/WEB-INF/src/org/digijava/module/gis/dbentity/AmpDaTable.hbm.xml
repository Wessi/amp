<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.gis.dbentity.AmpDaTable" table="AMP_DA_TABLE">
		<cache usage="read-write"/>
		<id column="ID" name="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">AMP_DA_TABLE_seq</param>
			</generator>
		</id>

        <property name="name" column="table_name" type="java.lang.String"/>
		<property name="code" column="table_code" type="java.lang.String"/>
		<property name="cssClass" column="css_class" type="java.lang.String"/>
		<property name="htmlStyle" column="html_style" type="java.lang.String"/>
		<property name="width" column="table_width" type="java.lang.String"/>

		<set name="columns" lazy="true" inverse="true" order-by="order_no asc" cascade="all-delete-orphan">
			<key column="widget_id"/>
			<one-to-many class="org.digijava.module.gis.dbentity.AmpDaColumn"/>
		</set>

		<set name="places" lazy="true" inverse="true" order-by="place_name asc" cascade="none">
			<key column="widget_id"/>
			<one-to-many class="org.digijava.module.gis.dbentity.AmpDaWidgetPlace"/>
		</set>
	</class>
</hibernate-mapping>
