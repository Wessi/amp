<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>
  <form-beans>
    <form-bean name="dmSettingsForm" type="org.digijava.module.dm.form.SettingsForm" />
  </form-beans>
  <action-mappings>
    <action name="dmSettingsForm" parameter="method" path="/dm/editSettings" scope="session" type="org.digijava.module.dm.action.EditSettings" unknown="true">
      <forward name="forward" path="/showLayout.do?layout=dm_editSettings" />
    </action>
  </action-mappings>
</struts-config>

