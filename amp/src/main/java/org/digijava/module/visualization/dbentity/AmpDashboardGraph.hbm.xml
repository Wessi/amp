<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.visualization.dbentity.AmpDashboardGraph" table="amp_dashboard_graph">
	  	<id name="id" column="id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">amp_dashboard_graph_seq</param>
			</generator>
		</id>
		
		<property name="order" column="order_" type="java.lang.Integer" />
		<many-to-one name="dashboard" column="dashboard_id"	class="org.digijava.module.visualization.dbentity.AmpDashboard" cascade="none" lazy="false"/>
		<many-to-one name="graph" column="graph_id"	class="org.digijava.module.visualization.dbentity.AmpGraph" cascade="none" lazy="false"/>

  	</class>
</hibernate-mapping>