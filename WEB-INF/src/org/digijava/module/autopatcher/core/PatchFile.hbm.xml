<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.autopatcher.core.PatchFile"
		table="PATCH_FILE">
		<cache usage="read-write" />
		<id name="id" column="id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">patch_file_id</param>
			</generator>
		</id>

		<property name="absolutePatchName" column="absolute_patch_name" type="java.lang.String" />
		<property name="invoked" column="invoked"
			type="java.sql.Timestamp" />
	</class>
</hibernate-mapping>
