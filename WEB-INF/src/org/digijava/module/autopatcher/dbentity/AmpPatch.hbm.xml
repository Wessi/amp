<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.autopatcher.dbentity.AmpPatch" table="AMP_PATCH">
		<cache usage="read-write" />
		<id name="id" column="id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_PATCH_seq</param>
			</generator>
		</id>

		<property name="keyName" column="key_name" type="java.lang.String"/>
		<property name="abstractFileName" column="abstract_file_name" type="java.lang.String"/>
		<property name="discovered" column="discovered" type="java.sql.Timestamp"/>
		<property name="lastInvoked" column="last_invoked" type="java.sql.Timestamp"/>
		<property name="md5" column="md5" type="java.lang.String"/>		
		
		
		<set name="logs" table="AMP_PATCH_LOG" cascade="all">
			<cache usage="read-write"/>
			<key column="amp_patch_id"/>
			<one-to-many class="org.digijava.module.autopatcher.dbentity.AmpPatchLog" />
		</set>
		
		
</class>