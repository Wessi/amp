<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>

	<class name="org.digijava.module.syndication.dbentity.CollectorFeedItem" table="DG_COLLECTOR_FEED_ITEM">
		<id column="ID" name="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">dg_collector_feed_item_seq</param>
			</generator>
		</id>

		<property name="siteId" column="SITE_ID"  type="java.lang.String"/>
		<property name="instanceId" column="MODULE_INSTANCE_ID"  type="java.lang.String"/>
		<property name="status" column="STATUS"  type="java.lang.Integer"/>
		<property name="schedule"  column="SHEDULE"  type="java.lang.Integer"/>
		<property name="scheduleTime"  column="SHEDULE_TIME"  type="java.util.Date"/>
		<property name="contentType" column="CONTENT_TYPE"  type="java.lang.String"/>
		<property name="firstRun" column="FIRST_RUN"  type="java.util.Date"/>
		<property name="lastRun" column="LAST_RUN"  type="java.util.Date"/>
		<property name="creationDate" column="CREATION_DATE"  type="java.util.Date"/>
        <many-to-one name="feed" column="FEED_ID" class="org.digijava.module.syndication.dbentity.CollectorFeed" cascade="none" />


	</class>

</hibernate-mapping>