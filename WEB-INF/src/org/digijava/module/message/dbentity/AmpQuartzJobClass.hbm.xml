<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.message.dbentity.AmpQuartzJobClass" table="AMP_QUARTZ_JOB_CLASS">

		<id name="id" column="jc_Id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_QUARTZ_JOB_CLASS_seq</param>
			</generator>
		</id>

		<property name="name" column="jc_name"  type="java.lang.String"/>
		<property name="classFullname" column="jc_class_fullname"  type="java.lang.String"/>
	</class>
</hibernate-mapping>
