<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.gis.dbentity.GisMapPoint" table="DG_GIS_MAP_POINT">
		<cache usage="read-write"/>
		<id column="ID" name="id" type="long" unsaved-value="0">
			<generator class="native">
				<param name="sequence">dg_gis_map_point_seq</param>
			</generator>
		</id>
                <many-to-one name="shape" column="SHAPE" class="org.digijava.module.gis.dbentity.GisMapShape" cascade="none"/>
                <property name="sequenceNumber" column="SEQ_NO" type="long"/>
                <property name="longatude" column="LONGATUDE" type="float"/>
                <property name="latitude" column="LATITUDE" type="float"/>
	</class>
</hibernate-mapping>
