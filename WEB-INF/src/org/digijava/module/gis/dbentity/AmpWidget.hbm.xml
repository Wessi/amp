<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.gis.dbentity.AmpWidget" table="AMP_DA_WIDGET" discriminator-value="w">
		<cache usage="read-write"/>
		<id column="ID" name="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">AMP_DA_WIDGET_seq</param>
			</generator>
		</id>

		<discriminator column="widget_type"/>
        <property name="name" column="widget_name" type="java.lang.String"/>
		<property name="code" column="widget_code" type="java.lang.String"/>
		<one-to-one name="place" class="org.digijava.module.gis.dbentity.AmpDaWidgetPlace" cascade="none"/>
		

		<subclass name="org.digijava.module.aim.dbentity.AmpIndicatorChartWidget" discriminator-value="indsecchart">
			<many-to-one name="indicator" column="ind_connect_id" class="org.digijava.module.aim.dbentity.AmpSector" cascade="none"/> 
		</subclass>			
		
	</class>
</hibernate-mapping>
