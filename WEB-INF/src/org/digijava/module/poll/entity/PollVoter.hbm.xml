<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.poll.entity.PollVoter" table="poll_voter">
	  	<cache usage="read-write"/>
		<id name="pollVoterId" type="long" column="poll_voter_id">
			<generator class="native">
				<param name="sequence">poll_voter_seq</param>
			</generator>
		</id>
		<property name="ipAddress" column="ip_address" type="string"/>
                <many-to-one name="poll" column="poll_id" class="org.digijava.module.poll.entity.Poll"/>
	</class>
</hibernate-mapping>
