<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>

	<class name="org.digijava.module.aim.dbentity.AmpFundingMTEFProjection" table="AMP_FUNDING_MTEF_PROJECTION">
		<cache usage="read-write" />
		<id name="ampFundingMTEFProjectionId" column="amp_fund_mtef_projection_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_FUND_mtef_projection_seq</param>
			</generator>
		</id>

		<property name="projected" column="projected"  type="java.lang.Long"/>
		<property name="projectionDate" column="projection_date" type="java.util.Date" />
		<property name="amount" column="amount"  type="java.lang.Double"/>
		
		<many-to-one name="ampCurrencyId" column="amp_currency_id" class="org.digijava.module.aim.dbentity.AmpCurrency" cascade="none"/>
		<many-to-one name="ampFundingId" column="amp_funding_id" class="org.digijava.module.aim.dbentity.AmpFunding" not-null="true"/>
	</class>
</hibernate-mapping>
