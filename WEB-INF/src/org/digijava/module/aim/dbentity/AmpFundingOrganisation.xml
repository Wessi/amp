<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpFundingOrganisation" table="AMP_FUNDING_ORGANISATION">
		<cache usage="read-write" />
		<id name="ampFundOrgId" column="amp_fund_org_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_FUNDING_ORGANISATION_seq</param>
			</generator>
		</id>
		
		<property name="active" column="active" type="java.lang.Boolean"/>
		<property name="delegatedPartner" column="delegated_partner" type="java.lang.Boolean"/>
		<property name="delegatedCooperation" column="delegated_cooperation" type="java.lang.Boolean"/>
		
			
		<many-to-one name="ampOrgId" column="amp_org_id" 
		class="org.digijava.module.aim.dbentity.AmpOrganisation" cascade="none"/>		
		
		<many-to-one name="ampActivityId" column="amp_activity_id" 
		class="org.digijava.module.aim.dbentity.AmpActivity" cascade="none"/>		
		
		
	</class>
	
</hibernate-mapping>

