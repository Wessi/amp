<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpModality" table="AMP_MODALITY">
		<cache usage="read-write" />
		<id name="ampModalityId" column="amp_modality_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_MODALITY_seq</param>
			</generator>
		</id>
		<property name="modalityCode" column="modality_code" type="java.lang.String"/>
		<property name="channelCode" column="channel_code" type="java.lang.String"/>
		<property name="type" column="type"  type="java.lang.String"/>
		<property name="description" column="description" type="org.digijava.kernel.db.SafeClobType"/>
		<property name="name" column="name"  type="java.lang.String"/>
		
	</class>
</hibernate-mapping>
