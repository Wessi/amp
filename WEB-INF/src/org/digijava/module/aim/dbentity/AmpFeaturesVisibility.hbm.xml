<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpFeaturesVisibility" table="AMP_FEATURES_VISIBILITY">
		<cache usage="read-write" />
		<id name="id" column="id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_FIELD_seq</param>
			</generator>
			</id>
		<property name="name" column="name" type="java.lang.String" />
		
    <set name="items" table="AMP_FIELDS_VISIBILITY" inverse="true" cascade="all-delete-orphan">
   		   <cache usage="read-write" />
           <key column="parent"/>
           <one-to-many class="org.digijava.module.aim.dbentity.AmpFieldsVisibility"/>
     </set>
		
	<many-to-one name="parent" column="parent" class="org.digijava.module.aim.dbentity.AmpModulesVisibility"/>
	
	<set name="templates" table="amp_features_templates" inverse="true">
		   <cache usage="read-write" />
           <key column="feature"/>
           <many-to-many column="template" class="org.digijava.module.aim.dbentity.AmpTemplatesVisibility"/>
     </set>

	</class>
</hibernate-mapping>
