<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpIndicatorSource" table="AMP_INDICATOR_SOURCE">
		<cache usage="read-write" />
		<id name="AmpIndicatorSourceId" column="amp_indicator_source_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_INDICATOR_SOURCE_seq</param>
			</generator>
		</id>
                <property name="sourceName" column="source_name" type="java.lang.String"/>
	</class>
</hibernate-mapping>
