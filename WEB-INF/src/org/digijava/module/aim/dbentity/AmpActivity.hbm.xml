		<!-- <many-to-one name="modality" column="amp_modality_id" class="org.digijava.module.aim.dbentity.AmpModality" cascade="none"/>  -->
		<many-to-one name="modality" column="amp_categ_val_modality_id" class="org.digijava.module.aim.dbentity.AmpCategoryValue" cascade="none"/>
		<many-to-one name="team" column="amp_team_id" class="org.digijava.module.aim.dbentity.AmpTeam" cascade="none"/>
		<many-to-one name="country" column="country_iso" class="org.digijava.kernel.dbentity.Country" cascade="none"/>

		<many-to-one name="author" column="author" class="org.digijava.kernel.user.User" cascade="none"/>   <!-- defunct -->
