		<property name="approvalStatus" column="approval_status" type="java.lang.String"/>

		<property name="lineMinRank" column="line_min_rank" type="java.lang.Integer"/>
		<property name="planMinRank" column="plan_min_rank" type="java.lang.Integer"/>



		<set name="categories" table="AMP_ACTIVITIES_CATEGORYVALUES" lazy="false" cascade="none">
			<cache usage="read-write"/>
			<key column="amp_activity_id" />
			<many-to-many class="org.digijava.module.aim.dbentity.AmpCategoryValue" column="amp_categoryvalue_id" />
		</set>

		<set name="activityDocuments" lazy="false" cascade="all-delete-orphan">
			<cache usage="read-write"/>
			<key column="amp_activity_id" />
			<one-to-many class="org.digijava.module.aim.dbentity.AmpActivityDocument"  />
		</set>

		<set name="referenceDocs" cascade="all-delete-orphan" inverse="true">
			<cache usage="read-write" />
			<key column="activity_id"/>
			<one-to-many class="org.digijava.module.aim.dbentity.AmpActivityReferenceDoc"/>
 		</set>

		<set name="orgrole" cascade="all-delete-orphan">
			<key column="activity"/>
			<one-to-many class="org.digijava.module.aim.dbentity.AmpOrgRole"/>
 		</set>

		<set name="sectors" cascade="all-delete-orphan">
			<cache usage="read-write"/>
			<key column="amp_activity_id"/>
			<one-to-many class="org.digijava.module.aim.dbentity.AmpActivitySector" />
		</set>

		<set name="componentes" cascade="all-delete-orphan">
			<key column="amp_activity_id"/>
			<one-to-many class="org.digijava.module.aim.dbentity.AmpActivityComponente" />
		</set>

		<set name="activityPrograms" table="AMP_ACTIVITY_THEME" cascade="none">
			<cache usage="read-write"/>
			<key column="amp_activity_id"/>
			<many-to-many class="org.digijava.module.aim.dbentity.AmpTheme" column="amp_theme_id"/>
		</set>

		<!-- set name="locations" table="AMP_ACTIVITY_LOCATION" lazy="true">
			<cache usage="read-write"/>
			<key column="amp_activity_id"/>
			<many-to-many class="org.digijava.module.aim.dbentity.AmpLocation" column="amp_location_id"/>
		</set-->

		<set name="locations" lazy="true" cascade="all-delete-orphan" inverse="true">
			<key column="amp_activity_id"/>
			<one-to-many class="org.digijava.module.aim.dbentity.AmpActivityLocation" />
		</set>

		<set name="costs" cascade="all-delete-orphan" inverse="true">
			<cache usage="read-write"/>
			<key column="amp_activity_id"/>
			<one-to-many class="org.digijava.module.aim.dbentity.EUActivity"/>
		</set>


		<set name="internalIds" table="AMP_ACTIVITY_INTERNAL_ID" >
			<key column="amp_activity_id"/>
			<composite-element class="org.digijava.module.aim.dbentity.AmpActivityInternalId">
				<many-to-one name="organisation" class="org.digijava.module.aim.dbentity.AmpOrganisation"
					column="amp_org_id"/>
				<property name="internalId" column="internal_id"  type="java.lang.String"/>
			</composite-element>
		</set>

		<set name="funding" cascade="all-delete-orphan" inverse="true">
		<cache usage="read-write" />
			<key column="amp_activity_id"/>
			<one-to-many class="org.digijava.module.aim.dbentity.AmpFunding"/>
		</set>

		<set name="components" table="AMP_ACTIVITY_COMPONENTS" lazy="true" cascade="none">
		<cache usage="read-write" />
			<key column="amp_activity_id"/>
			<many-to-many class="org.digijava.module.aim.dbentity.AmpComponent" column="amp_component_id"/>
 		</set>
 		<set name="documents" table="AMP_ACTIVITY_KM_DOCUMENTS" lazy="true" cascade="none">
  	            <cache usage="read-write"/>
				<key column="amp_activity_id"/>
				<many-to-many class="org.digijava.module.cms.dbentity.CMSContentItem" column="id"/>
		</set>

		<set name="member" table="AMP_MEMBER_ACTIVITIES" lazy="true">
				<key column="amp_activity_id"/>
				<many-to-many class="org.digijava.module.aim.dbentity.AmpTeamMember" column="amp_member_id"/>
		</set>

		<set name="notes" cascade="delete" inverse="true">
		<cache usage="read-write" />
			<key column="amp_activity_id"/>
			<one-to-many class="org.digijava.module.aim.dbentity.AmpNotes"/>
		</set>

		<set name="closingDates" cascade="delete" order-by="closing_date desc" inverse="true">
		<cache usage="read-write" />
			<key column="amp_activity_id"/>
			<one-to-many class="org.digijava.module.aim.dbentity.AmpActivityClosingDates" />
		</set>

		<set name="issues" cascade="all-delete-orphan" order-by="amp_issue_id asc" inverse="true">
		<cache usage="read-write" />
			<key column="amp_activity_id"/>
			<one-to-many class="org.digijava.module.aim.dbentity.AmpIssues"/>
 		</set>

		<set name="regionalFundings" cascade="all-delete-orphan" inverse="true">
		<cache usage="read-write" />
			<key column="activity_id"/>
			<one-to-many class="org.digijava.module.aim.dbentity.AmpRegionalFunding"/>
 		</set>

 		<set name="survey" cascade="save-update" table="AMP_AHSURVEY" inverse="true">
			<cache usage="read-write"/>
			<key column="amp_activity_id"/>
			<one-to-many class="org.digijava.module.aim.dbentity.AmpAhsurvey"/>
		</set>
            <set name="actPrograms" cascade="all" inverse="true">
			<cache usage="read-write"/>
			<key column="amp_activity_id"/>
			<one-to-many class="org.digijava.module.aim.dbentity.AmpActivityProgram"/>
       </set>
      
      <set name="indicators" table="AMP_INDICATOR_PROJECT" lazy="true" cascade="none">
			<cache usage="read-write"/> 
			<key column="amp_activity_id"/>
			<many-to-many class="org.digijava.module.aim.dbentity.AmpIndicator" column="indicator_id"/>
	  </set>

	</class>
</hibernate-mapping>
