<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.kernel.entity.OracleLocale" table="J_LOCALES">
        <cache usage="read-write"/>
		<id column="LOCALE_ABBREV" name="oracleLocale" type="java.lang.String" unsaved-value="null">
			<generator class="assigned" />
		</id>
		<many-to-one name="locale" column="ISO" class="org.digijava.kernel.entity.Locale" cascade="none"/>
        <property column="NLS_LANG_NAME" name="languageName" type="java.lang.String"/>
	</class>
</hibernate-mapping>
